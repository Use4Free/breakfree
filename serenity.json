{
  "cache_file": "serenity.db",
  "log": {
    "level": "debug",
    "timestamp": true
  },
  "listen": "0.0.0.0:8080",
  "subscriptions": [
    {
      "name": "aggregator-lite",
      "url": "https://raw.githubusercontent.com/mahdibland/V2RayAggregator/master/Eternity.yml",
      "deduplication": true,
      "process": [
        {
          "exclude": [
            "AE",
            "BH",
            "CN",
            "IL",
            "IR",
            "RU",
            "TR"
          ],
          "exclude_outbound_type": [
            "http",
            "socks",
            "shadowsocksr"
          ]
        }
      ]
    },
    {
      "name": "aggregator",
      "url": "https://raw.githubusercontent.com/mahdibland/V2RayAggregator/master/sub/sub_merge_yaml.yml",
      "deduplication": true,
      "process": [
        {
          "exclude": [
            "AE",
            "BH",
            "CN",
            "IL",
            "IR",
            "RU",
            "TR"
          ],
          "exclude_outbound_type": [
            "http",
            "socks",
            "shadowsocksr"
          ]
        }
      ]
    },
    {
      "name": "yebekhe",
      "url": "https://raw.githubusercontent.com/yebekhe/TVC/main/subscriptions/singbox/mix.json",
      "deduplication": true,
      "process": [
        {
          "exclude": [
            "AE",
            "BH",
            "CN",
            "IL",
            "IR",
            "RU",
            "TR"
          ],
          "exclude_outbound_type": [
            "http",
            "socks",
            "shadowsocksr"
          ]
        }
      ]
    }
  ],
  "templates": [
    {
      "name": "breakfree",
      "disable_default_rules": true
    }
  ],
  "profiles": [
    {
      "subscription": [
        "yebekhe",
        "aggregator"
      ],
      "template": "breakfree",
      "name": "servers"
    },
    {
      "subscription": [
        "yebekhe",
        "aggregator-lite"
      ],
      "template": "breakfree",
      "name": "servers-lite"
    }
  ]
}
